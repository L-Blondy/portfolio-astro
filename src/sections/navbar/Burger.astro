---
import type { HTMLAttributes } from 'astro/types';

interface Props extends HTMLAttributes<'button'> {}
---

<button {...Astro.props}>
   <div class="h-16 w-16 flex items-center justify-center -mr-4">
      <svg
         class="text-dark-primary"
         width="36"
         height="36"
         viewBox="0 0 36 36"
         fill="none"
         xmlns="http://www.w3.org/2000/svg"
      >
         <rect y="5" width="36" height="3" fill="currentColor"></rect>
         <rect x="12" y="17" width="24" height="3" fill="currentColor"></rect>
         <rect x="24" y="29" width="12" height="3" fill="currentColor"></rect>
      </svg>
   </div>
</button>
